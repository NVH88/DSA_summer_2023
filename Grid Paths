#include<bits/stdc++.h>
using namespace std;
const int M = 1e9 + 7;

int main() {
    ios_base::sync_with_stdio(false); cin.tie(NULL);
    #ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
    #endif
   
    int n; cin >> n; char c[n + 1][n + 1]; int dp[n + 1][n + 1];
    memset(dp, 0, sizeof(dp));
    for(int i = 1; i <= n; ++i) 
        for(int j = 1; j <= n; ++j)
            cin >> c[i][j];

    if(c[1][1] == '*') {
        cout << 0; return 0;
    }
    dp[1][1] = 1;
    for(int i = 0; i <= n; ++i) c[0][i] = '*', c[i][0] = '*';
    for(int i = 1; i <= n; ++i) {
        for(int j = 1; j <= n; ++j) {
            if(c[i][j] != '*') {
                dp[i][j] += dp[i - 1][j] + dp[i][j - 1];
                dp[i][j] %= M;
            }
        }
    }
    cout << dp[n][n];
}
