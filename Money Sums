#include<bits/stdc++.h>
using namespace std;

int main() {
    ios_base::sync_with_stdio(false); cin.tie(NULL);
    #ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
    #endif
    
    int n, s = 0; cin >> n;
    int a[n]; for(int i = 0; i < n; ++i) cin >> a[i], s += a[i];
    int f[s + 1] = {0}; f[0] = 1;
    vector<int> v;

    for(int i = 0; i < n; ++i) {
        for(int j = s; j >= a[i]; --j) 
            if(f[j - a[i]]) f[j] = 1;
    }

    for(int i = 1; i <= s; ++i)
        if(f[i]) v.push_back(i);

    cout << v.size() << endl;
    for(int i : v) cout << i << " ";
}
