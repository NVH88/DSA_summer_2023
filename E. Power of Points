#include<bits/stdc++.h>
using namespace std;

int main() {
    ios_base::sync_with_stdio(false); cin.tie(NULL);
    #ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
    #endif
    
    int t; cin >> t;
    while(t--) {
        int n; cin >> n; int a[n];
        map<int, int> mp; vector<pair<int, int>> v;
        for(int i = 0; i < n; ++i) {
            cin >> a[i]; ++mp[a[i]];
        }
        for(pair<int, int> pi : mp) v.push_back(pi);
        int sz = v.size();
        long long fl[sz], fr[sz], sl[sz], sr[sz];
        fl[0] = v[0].second; fr[sz - 1] = v[sz - 1].second; sl[0] = 0; sr[sz - 1] = 0;
        for(int i = 1; i < sz; ++i) fl[i] = fl[i - 1] + v[i].second;
        for(int i = sz - 2; i >= 0; --i) fr[i] = fr[i + 1] + v[i].second;
        for(int i = 1; i < sz; ++i) {
            int x = v[i].first - 1;
            sl[i] = sl[i - 1] + fl[i - 1] * (x - v[i - 1].first + 1);
        }
        for(int i = sz - 2; i >= 0; --i) {
            int x = v[i].first + 1;
            sr[i] = sr[i + 1] + fr[i + 1] * (v[i + 1].first - x + 1);
        }
        map<int, long long> ans;
        for(int i = 0; i < sz; ++i) {
            long long x = sl[i] + fl[i] + sr[i] + fr[i] - v[i].second;
            ans[v[i].first] = x;
        }
        for(int i = 0; i < n; ++i) cout << ans[a[i]] << " ";
        cout << endl;
    }
}
