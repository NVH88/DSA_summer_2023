#include<bits/stdc++.h>
using namespace std;

int main() {
    ios_base::sync_with_stdio(false); cin.tie(NULL);
    #ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
    #endif
    
    int n; cin >> n;
    vector<int> v;
    for(int i = 0; i < n; ++i) {
        int x; cin >> x;
        auto it = lower_bound(v.begin(), v.end(), x);
        if(it == v.end()) v.push_back(x);
        else *it = x;
    }
    cout << v.size();
}
